{
	"name": "ejjy-pc-based-scale",
	"productName": "EJJY PC-Based Scale",
	"version": "1.1.28",
	"private": true,
	"homepage": "./",
	"main": "./electron/main.js",
	"scripts": {
		"lint": "cd src && eslint . --ext .tsx --ext .ts",
		"lint:fix": "cd src && eslint . --ext .tsx --ext .ts --fix",
		"prettify": "prettier --write ./src",
		"start": "concurrently \"react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
		"build": "react-scripts build",
		"server": "node public/js/server.js",
		"electron": "wait-on http://localhost:3000 && electron .",
		"pack:clean": "rm -rf build && rm -rf dist/build",
		"pack:build": "npm run pack:clean && npm run build && mv build ./dist/build && electron-packager ./dist EJJY-PC-Based-Scale --platform=win32 --overwrite",
		"pack:install": "cd EJJY-PC-Based-Scale-win32-x64/resources/app && npm install",
		"pack:zip": "7z a -tzip EJJY-PC-Based-Scale.zip EJJY-PC-Based-Scale-win32-x64",
		"pack": "npm version patch --no-git-tag-version && npm run pack:build && npm run pack:install && npm run pack:zip"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"antd": "^4.10.0",
		"axios": "^0.20.0",
		"classnames": "^2.2.6",
		"connected-react-router": "^6.8.0",
		"cors": "^2.8.5",
		"dayjs": "^1.9.4",
		"electron-is-dev": "^1.2.0",
		"formik": "^2.1.5",
		"history": "^4.10.1",
		"lodash": "^4.17.20",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-redux": "^7.2.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.3",
		"redux": "^4.1.0",
		"redux-actions": "^2.6.5",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.3",
		"reselect": "^4.0.0",
		"typescript": "^3.7.5",
		"validator": "^13.1.17",
		"yup": "^0.29.3"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/classnames": "^2.2.10",
		"@types/lodash": "^4.14.161",
		"@types/node": "^12.12.55",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@types/react-redux": "^7.1.9",
		"@types/react-router-dom": "^5.1.5",
		"@types/redux-actions": "^2.6.1",
		"@types/yup": "^0.29.6",
		"concurrently": "^5.3.0",
		"electron": "^11.2.1",
		"electron-packager": "^15.2.0",
		"express": "^4.17.1",
		"node-sass": "^4.14.1",
		"wait-on": "^5.2.1",
		"prettier": "^2.3.2",
		"eslint": "^7.29.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"@typescript-eslint/eslint-plugin": "^4.28.1",
		"@typescript-eslint/parser": "^4.28.1"
	}
}
